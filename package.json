{
  "name": "wxt-svelte-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:verbose": "bun test --verbose",
    "test:webrtc": "bun test src/entrypoints/offscreen/webrtc.test.ts",
    "dev": "bun run scripts/dev.ts",
    "dev:simple": "bun run build:wasm && wxt",
    "dev:firefox": "bun run build:wasm && wxt -b firefox",
    "dev:edge": "bun run build:wasm && wxt -b edge",
    "build": "bun run build:wasm && wxt build",
    "build:firefox": "bun run build:wasm && wxt build -b firefox",
    "build:edge": "bun run build:wasm && wxt build -b edge",
    "build:wasm": "cargo build --target wasm32-unknown-unknown --release && wasm-pack build --target web --out-dir pkg",
    "build:wasm:dev": "cargo build --target wasm32-unknown-unknown && wasm-pack build --target web --out-dir pkg --dev",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "type-check": "bun run tsc --noEmit",
    "lint": "bun run check",
    "clean": "rm -rf .wxt dist pkg/mpc_wallet* target/wasm32-unknown-unknown",
    "fresh": "bun run clean && bun install && bun run build:wasm",
    "benchmark": "bun run scripts/benchmark.ts",
    "perf": "bun run scripts/performance.ts",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "@types/bun": "^1.2.15",
    "@types/chrome": "^0.0.326",
    "@types/node": "^22.15.30",
    "@types/uuid": "^10.0.0",
    "@wxt-dev/module-svelte": "^2.0.3",
    "bun-types": "^1.2.15",
    "svelte": "^5.33.14",
    "svelte-check": "^4.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "wxt": "^0.20.7"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "tailwindcss": "^4.1.8",
    "tonkotsu": "^1.0.17",
    "uuid": "^11.1.0",
    "viem": "^2.30.6",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}